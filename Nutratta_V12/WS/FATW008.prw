//************************************************************************************************************
WsMethod BusProdut WsReceive cWSPWD,cSeek WsSend oProduto WsService FATWPROD
//************************************************************************************************************
Local cChave 	:= ::cWSPWD  //CHAVE DE VALIDAO ORIGEM
Local cPARPWD	:= '123456'//AllTrim(GetMv("MV_PWINTOP"))
Private aProduto	:={}


// VALIDA SE CHAVE ESTÁ CORRETA
If AllTrim(cChave) == AllTrim(cPARPWD)
			
	//--------------------------------------------------------------------------------
	//Posiciona na DA1 para buscaro preço do produto.
	//--------------------------------------------------------------------------------
	dbSelectArea("DA1")
	dbSetOrder(7)//+Padr(cCodTab,3)+Padr(cProduto,15)+Padr(cCond,3))  //DA1_FILIAL+DA1_CODTAB+DA1_CODPRO+DA1_COND   
	If dbSeek(xFilial("DA1")+cSeek)  //DA1_FILIAL+DA1_CODTAB+DA1_CODPRO+DA1_COND 
         //::nPrc:= Str(DA1->DA1_0000)
         AADD(aProduto,{Str(DA1->DA1_0000)  })         
    Else
    	conOut("[ERRO] FATW07 " + dtoc(date()) + " " + Time() +"==>Preço nao encontrado!")
		//::nPrc := Str(0)
		AADD(aProduto,{Str(0)})
    EndIf

	For nX:=1 to Len(aProduto)
		AADD (oProduto,LoadORet(aProduto[nX]))
	Next
					
Else
	conOut("[ERRO] POOMSM04.PRW " + dtoc(date()) + " " + Time() +"==>Chave de segurança inválida!")
	AADD (oProduto,LoadORet(aProduto))
EndIf

Return(.T.)


